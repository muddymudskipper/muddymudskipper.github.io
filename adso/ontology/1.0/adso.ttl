@prefix : <https://w3id.org/adso/ontology/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <http://www.geonames.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <https://w3id.org/adso/ontology/> .

<https://w3id.org/adso/ontology/> rdf:type owl:Ontology ;
                                   owl:imports <http://www.geonames.org/ontology> ,
                                               <https://www.w3.org/ns/prov> ,
                                               <http://qudt.org/2.0/schema/SCHEMA_QUDT-v2.0.ttl> ;
                                   dcterms:title "The Agave and Dasyirion Spirit Ontology (ADSO)" ;
                                   dc:creator "Thomas Wilmering" ;
                                   dc:description "The Agave and Dasyirion Spirit Ontology (ADSO) provides concepts and properties for the description of distilled beverages based on cooked agave and dasylirion." ;
                                   dcterms:license <http://creativecommons.org/licenses/by-sa/4.0/> ;
                                   owl:versionInfo "Revision: 1.0b1" ;
                                   dc:contributor "G. Fazekas" ;
                                   dc:rights "This ontology is distributed under a Creative Commons License - http://creativecommons.org/licenses/by-sa/4.0/" ;
                                   dcterms:date "2018-04-08"^^xsd:date .

#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/adso/ontology/additive
:additive rdf:type owl:ObjectProperty ;
          rdfs:domain :AddingDistillateAdditive ,
                      :Flavoring ;
          rdfs:range :Additive ;
          rdfs:comment "links additives used for flavoring of a distillate before or after final distillation" ;
          rdfs:label "additive" .


###  https://w3id.org/adso/ontology/amount
:amount rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf qudt:value ,
                           owl:topObjectProperty ;
        rdfs:domain :Specification ;
        rdfs:range :Amount ;
        rdfs:comment "specifies the amount of a compound in a spirit's specifications" ;
        rdfs:label "amount" .


###  https://w3id.org/adso/ontology/amount_of
:amount_of rdf:type owl:ObjectProperty ;
           rdfs:domain :Amount ;
           rdfs:range :DistillateProduct ;
           rdfs:comment "associates instances of Amount to a partial distillate" ;
           rdfs:label "amount_of" .


###  https://w3id.org/adso/ontology/aroma
:aroma rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :sensory_charasteristic ;
       rdfs:comment "links tasting notes to an aroma" ;
       rdfs:label "aroma" .


###  https://w3id.org/adso/ontology/available_in
:available_in rdf:type owl:ObjectProperty ;
              rdfs:domain :AgaveDasylirionSpirit ;
              rdfs:range :Bottle ;
              rdfs:comment "associates a spirit with a distribution container, e.g. a glass bottle" ;
              rdfs:label "available_in" .


###  https://w3id.org/adso/ontology/average_atmospheric_pressure
:average_atmospheric_pressure rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf qudt:value ,
                                                 :weather_attribute ;
                              rdfs:comment "average atmospheric pressure" ;
                              rdfs:label "average_atmospheric_pressure" .


###  https://w3id.org/adso/ontology/average_humidity
:average_humidity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf qudt:value ,
                                     :weather_attribute ;
                  rdfs:comment "average humidity" ;
                  rdfs:label "average_humidity" .


###  https://w3id.org/adso/ontology/average_precipitation
:average_precipitation rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf qudt:value ,
                                          :weather_attribute ;
                       rdfs:comment "average precipitation" ;
                       rdfs:label "average_precipitation" .


###  https://w3id.org/adso/ontology/average_temperature
:average_temperature rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf qudt:value ,
                                        :weather_attribute ;
                     rdfs:comment "average temperature" ;
                     rdfs:label "average_temperature" .


###  https://w3id.org/adso/ontology/begins_at_volume
:begins_at_volume rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf qudt:value ;
                  rdfs:domain :PartialDistillate ;
                  rdfs:range qudt:QuantityValue ;
                  rdfs:comment "specifies the beginning of a partial distillate in volume" ;
                  rdfs:label "amount_obegins_at_volume" .


###  https://w3id.org/adso/ontology/bottle
:bottle rdf:type owl:ObjectProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Bottling ;
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty :bottle ;
                     owl:someValuesFrom :Bottle
                   ] ;
        rdfs:comment "associates the bottling task with a bottle" ;
        rdfs:label "bottle" .


###  https://w3id.org/adso/ontology/brand
:brand rdf:type owl:ObjectProperty ;
       rdfs:domain :AgaveDasylirionSpirit ;
       rdfs:range :Brand ;
       rdfs:comment "associates a spirit with the brand" ;
       rdfs:label "brand" .


###  https://w3id.org/adso/ontology/brand_distillery
:brand_distillery rdf:type owl:ObjectProperty ;
                  rdfs:domain :Brand ;
                  rdfs:range :Distillery ;
                  rdfs:comment "associates a brand with a distillery" ;
                  rdfs:label "brand_distillery" .


###  https://w3id.org/adso/ontology/climate
:climate rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :location_attribute ;
         rdfs:domain :Location ;
         rdfs:comment "specifies the climate in the region of a location" ;
         rdfs:label "climate" .


###  https://w3id.org/adso/ontology/company
:company rdf:type owl:ObjectProperty ;
         rdfs:domain :Brand ;
         rdfs:range :Company ;
         rdfs:comment "associates a brand with a company" ;
         rdfs:label "company" .


###  https://w3id.org/adso/ontology/compound
:compound rdf:type owl:ObjectProperty ;
          rdfs:domain :Specification ;
          rdfs:range :Compound ;
          rdfs:comment "links a specification to a compound found in a spirit" ;
          rdfs:label "compound" .


###  https://w3id.org/adso/ontology/distillery
:distillery rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Distillation ;
            rdfs:range :Distillery ;
            rdfs:comment "links the distillation task to a distillery" ;
            rdfs:label "distillery" .


###  https://w3id.org/adso/ontology/elevation
:elevation rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf qudt:value ,
                              :location_attribute ;
           rdfs:comment "specifies the elevation of a location" ;
           rdfs:label "elevation" .


###  https://w3id.org/adso/ontology/ends_at_volume
:ends_at_volume rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf qudt:value ;
                rdfs:domain :PartialDistillate ;
                rdfs:range qudt:QuantityValue ;
                rdfs:comment "specifies the end of a partial distillate in volume" ;
                rdfs:label "ends_at_volume" .


###  https://w3id.org/adso/ontology/geonames_feature
:geonames_feature rdf:type owl:ObjectProperty ;
                  rdfs:domain :Location ;
                  rdfs:range geo:Feature ;
                  rdfs:comment "links a location with a GeoNames Feature" ;
                  rdfs:label "geonames_feature" .


###  https://w3id.org/adso/ontology/has_volume
:has_volume rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf qudt:value ;
            rdfs:domain :DistillateProduct ,
                        :FermentedJuice ,
                        :Juice ,
                        :LiquidAdditive ;
            rdfs:comment "specifies the volume of a liquid" ;
            rdfs:label "has_volume" .


###  https://w3id.org/adso/ontology/label
:label rdf:type owl:ObjectProperty ;
       rdfs:domain :Bottle ;
       rdfs:range :Label ;
       rdfs:comment "links a bottle with its label." ;
       rdfs:label "label" .


###  https://w3id.org/adso/ontology/location
:location rdf:type owl:ObjectProperty ;
          rdfs:domain :Task ;
          rdfs:range :Location ;
          rdfs:comment "links a task with its location" ;
          rdfs:label "location" .


###  https://w3id.org/adso/ontology/location_attribute
:location_attribute rdf:type owl:ObjectProperty ;
                    rdfs:domain :Location ;
                    rdfs:comment "specifies an attribute for the description of a location" ;
                    rdfs:label "location_attribute" .


###  https://w3id.org/adso/ontology/material
:material rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Tool ;
          rdfs:range :Material ;
          rdfs:comment "specifies the material of a production tool" ;
          rdfs:label "material" .


###  https://w3id.org/adso/ontology/maturity
:maturity rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf qudt:value ;
          rdfs:domain :Plant ;
          rdfs:comment "specifies the maturity of  plant" ;
          rdfs:label "maturity" .


###  https://w3id.org/adso/ontology/part_of
:part_of rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :PartialDistillate ;
         rdfs:range :PureDistillate ;
         rdfs:comment "links a partial distillate to the complete" ;
         rdfs:label "part_of" .


###  https://w3id.org/adso/ontology/plant_variety
:plant_variety rdf:type owl:ObjectProperty ;
               rdfs:domain :Plant ;
               rdfs:range :PlantVariety ;
               rdfs:comment "associates a plant with its variety" ;
               rdfs:label "plant_variety" .


###  https://w3id.org/adso/ontology/process
:process rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:domain :AgaveDasylirionSpirit ;
         rdfs:range :Process ;
         rdfs:comment "links a spitit with its production process" ;
         rdfs:label "process" .


###  https://w3id.org/adso/ontology/product
:product rdf:type owl:ObjectProperty ;
         rdfs:domain :AgaveDasylirionSpirit ;
         rdfs:range :DistillateProduct ;
         rdfs:comment "links a distributed product with a distillate produced in the prodcution process" ;
         rdfs:label "product" .


###  https://w3id.org/adso/ontology/purity
:purity rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf qudt:value ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :AgaveDasylirionSpirit ;
        rdfs:comment "percentage of agave/dasylirion product in 'Mixto'" ;
        rdfs:label "purity" .


###  https://w3id.org/adso/ontology/sensory_charasteristic
:sensory_charasteristic rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain :TastingNotes ;
                        rdfs:comment "links tasting note to sensory characteristics" ;
                        rdfs:label "sensory_characteristic" .


###  https://w3id.org/adso/ontology/slope
:slope rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf qudt:value ,
                          :location_attribute ;
       rdfs:comment "specifies the slope of a location" ;
       rdfs:label "slope" .


###  https://w3id.org/adso/ontology/slope_orientation
:slope_orientation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :location_attribute ;
                   rdfs:comment "specifies the slope orientation of a location" ;
                   rdfs:label "slope_orientation" .


###  https://w3id.org/adso/ontology/soil_properties
:soil_properties rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :location_attribute ;
                 rdfs:comment "specifies the soil properties of a location" ;
                 rdfs:label "soil_properties" .


###  https://w3id.org/adso/ontology/tactile
:tactile rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :sensory_charasteristic ;
         rdfs:comment "links tasting notes to tactile charasterstics" ;
         rdfs:label "tactile" .


###  https://w3id.org/adso/ontology/task
:task rdf:type owl:ObjectProperty ,
               owl:InverseFunctionalProperty ;
      rdfs:domain :Process ;
      rdfs:range :Task ;
      rdfs:comment "links the process to individual tasks" ;
      rdfs:label "task" .


###  https://w3id.org/adso/ontology/tool
:tool rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf <http://www.w3.org/ns/prov#used> ;
      rdf:type owl:FunctionalProperty ;
      rdfs:domain :Task ;
      rdfs:range :Tool ;
      rdfs:label "tool" .


###  https://w3id.org/adso/ontology/used_amount
:used_amount rdf:type owl:ObjectProperty ;
             rdfs:domain :Blending ;
             rdfs:range :Amount ;
             rdfs:comment "specifies the amount of a distillate used in blending" ;
             rdfs:label "used_amount" .


###  https://w3id.org/adso/ontology/visual
:visual rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :sensory_charasteristic ;
        rdfs:comment "links tasting notes to visual characteristics" ;
        rdfs:label "visual" .


###  https://w3id.org/adso/ontology/volume
:volume rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf qudt:value ;
        rdfs:domain :Amount ;
        rdfs:range qudt:QuantityValue ;
        rdfs:comment "links instances of Amount to a value" ;
        rdfs:label "volume" .


###  https://w3id.org/adso/ontology/weather_attribute
:weather_attribute rdf:type owl:ObjectProperty ;
                   rdfs:domain :WeatherCondition ;
                   rdfs:comment "links the weather condition to individual factors" ;
                   rdfs:label "weather_attribute" .


###  https://w3id.org/adso/ontology/weather_conditions
:weather_conditions rdf:type owl:ObjectProperty ;
                    rdfs:domain :Task ;
                    rdfs:range :WeatherCondition ;
                    rdfs:comment "associates a task with a weather condition" ;
                    rdfs:label "weather_conditions" .


###  https://w3id.org/adso/ontology/yeast
:yeast rdf:type owl:ObjectProperty ;
       rdfs:domain :Fermentation ;
       rdfs:comment "specifies the type of yeast used in the fermentation" ;
       rdfs:label "yeast" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/adso/ontology/is_organic
:is_organic rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :AgaveDasylirionSpirit ;
            rdfs:range xsd:boolean ;
            rdfs:comment "specifies if a spirit is organic" ;
            rdfs:label "is_organic" .


###  https://w3id.org/adso/ontology/nom
:nom rdf:type owl:DatatypeProperty ,
              owl:FunctionalProperty ;
     rdfs:domain :Distillery ;
     rdfs:range xsd:nonNegativeInteger ;
     rdfs:comment "associates a distillery with it's designated NOM number" ;
     rdfs:label "nom" .


###  https://w3id.org/adso/ontology/notes
:notes rdf:type owl:DatatypeProperty ;
       rdfs:domain :TastingNotes ;
       rdfs:range xsd:string ;
       rdfs:comment "links TastingNotes to text" ;
       rdfs:label "notes" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/adso/ontology/AddingDistillateAdditive
:AddingDistillateAdditive rdf:type owl:Class ;
                          rdfs:subClassOf :Task ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                            owl:someValuesFrom :DistillateWithAdditives
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                            owl:someValuesFrom :Additive
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                            owl:someValuesFrom :DistillateProduct
                                          ] ;
                          rdfs:comment "Additives added to distillate for further distillation" ;
                          rdfs:label "AddingDistillateAdditive" .


###  https://w3id.org/adso/ontology/Additive
:Additive rdf:type owl:Class ;
          rdfs:subClassOf :TaskProduct ;
          rdfs:comment "Additive added to a distillate before or after the last distillation" ;
          rdfs:label "Additive" .


###  https://w3id.org/adso/ontology/Agave
:Agave rdf:type owl:Class ;
       rdfs:subClassOf :PlantVariety ;
       rdfs:comment "Agave variety" ;
       rdfs:label "Agave" .


###  https://w3id.org/adso/ontology/AgaveDasylirionSpirit
:AgaveDasylirionSpirit rdf:type owl:Class ;
                       rdfs:comment "A spirit made from cooked agave or dasylirion" ;
                       rdfs:label "AgaveDasylirionSpirit" .


###  https://w3id.org/adso/ontology/AgaveSpirit
:AgaveSpirit rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :AgaveDasylirionSpirit
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :process ;
                                                          owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :task ;
                                                                               owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                    owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                                                                                    owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                         owl:onProperty :plant_variety ;
                                                                                                                         owl:someValuesFrom :Agave
                                                                                                                       ]
                                                                                                  ]
                                                                             ]
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "A spirit made from cooked agave." ;
             rdfs:label "AgaveSpirit" .


###  https://w3id.org/adso/ontology/AgedDistillate
:AgedDistillate rdf:type owl:Class ;
                rdfs:subClassOf :DistillateProduct ;
                rdfs:comment "aged distillate" ;
                rdfs:label "AgedDistillate" .


###  https://w3id.org/adso/ontology/Ageing
:Ageing rdf:type owl:Class ;
        rdfs:subClassOf :Task ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                          owl:someValuesFrom :AgedDistillate
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://www.w3.org/ns/prov#used> ;
                          owl:someValuesFrom :DistillateProduct
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :tool ;
                          owl:someValuesFrom :AgeingContainer
                        ] ;
        rdfs:comment "Ageing of a distillate" .


###  https://w3id.org/adso/ontology/AgeingContainer
:AgeingContainer rdf:type owl:Class ;
                 rdfs:subClassOf :Tool ;
                 rdfs:comment "Container for aging a distillate." ;
                 rdfs:label "AgeingContainer" .


###  https://w3id.org/adso/ontology/Aldehydes
:Aldehydes rdf:type owl:Class ;
           rdfs:subClassOf :Compound ;
           rdfs:label "Aldehydes" .


###  https://w3id.org/adso/ontology/Amount
:Amount rdf:type owl:Class ;
        rdfs:comment "An amount of a partial distillate." ;
        rdfs:label "Amount" .


###  https://w3id.org/adso/ontology/Aroma
:Aroma rdf:type owl:Class ;
       rdfs:subClassOf :SensoryCharacteristic ;
       owl:disjointWith :TactileCharacteristic ,
                        :VisualCharacteristic ;
       rdfs:comment "An aroma described in tasting notes." ;
       rdfs:label "Aroma" .


###  https://w3id.org/adso/ontology/Autoclave
:Autoclave rdf:type owl:Class ;
           rdfs:subClassOf :Oven ;
           rdfs:comment "autoclave" ;
           rdfs:label "Autoclave" .


###  https://w3id.org/adso/ontology/Bagasse
:Bagasse rdf:type owl:Class ;
         rdfs:subClassOf :TaskProduct ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( :CrushedPlantProduct
                                         :FermentationProduct
                                       )
                         ] ;
         rdfs:comment "bagasse" ;
         rdfs:label "Bagasse" .


###  https://w3id.org/adso/ontology/BlendedDistillate
:BlendedDistillate rdf:type owl:Class ;
                   rdfs:subClassOf :DistillateProduct ;
                   rdfs:comment "blended distillate" ;
                   rdfs:label "BlendedDistillate" .


###  https://w3id.org/adso/ontology/Blending
:Blending rdf:type owl:Class ;
          rdfs:subClassOf :Task ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                            owl:someValuesFrom :BlendedDistillate
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/prov#used> ;
                            owl:someValuesFrom :PartialDistillate
                          ] ;
          rdfs:comment "blending of different distillates, can also be used to specify colas/puntas share" ;
          rdfs:label "Blending" .


###  https://w3id.org/adso/ontology/Bottle
:Bottle rdf:type owl:Class ;
        rdfs:comment "A bottle for spirit distribution." ;
        rdfs:label "Bottle" .


###  https://w3id.org/adso/ontology/BottledSpirit
:BottledSpirit rdf:type owl:Class ;
               rdfs:subClassOf :TaskProduct ;
               rdfs:comment "bottled spirit" ;
               rdfs:label "BottledSpirit" .


###  https://w3id.org/adso/ontology/Bottling
:Bottling rdf:type owl:Class ;
          rdfs:subClassOf :Task ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                            owl:someValuesFrom :BottledSpirit
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/prov#used> ;
                            owl:someValuesFrom :DistillateProduct
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :bottle ;
                            owl:someValuesFrom :Bottle
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :tool ;
                            owl:someValuesFrom :BottlingTool
                          ] ;
          rdfs:comment "bottling a distillate for distribution" ;
          rdfs:label "Bottling" .


###  https://w3id.org/adso/ontology/BottlingTool
:BottlingTool rdf:type owl:Class ;
              rdfs:subClassOf :Tool ;
              rdfs:comment "Tool or machine used for bottling. Bottling may also be a manual task." ;
              rdfs:label "BottlingTool" .


###  https://w3id.org/adso/ontology/Brand
:Brand rdf:type owl:Class ;
       rdfs:comment "A brand of a spirit." ;
       rdfs:label "Brand" .


###  https://w3id.org/adso/ontology/BrickOven
:BrickOven rdf:type owl:Class ;
           rdfs:subClassOf :Oven ;
           rdfs:comment "brick oven" ;
           rdfs:label "BrickOven" .


###  https://w3id.org/adso/ontology/ChileanMill
:ChileanMill rdf:type owl:Class ;
             rdfs:subClassOf :Mill ;
             rdfs:comment "Chilean mill" ;
             rdfs:label "ChileanMill" .


###  https://w3id.org/adso/ontology/Clay
:Clay rdf:type owl:Class ;
      rdfs:subClassOf :Material ;
      rdfs:comment "Clay material." ;
      rdfs:label "Clay" .


###  https://w3id.org/adso/ontology/ClayOven
:ClayOven rdf:type owl:Class ;
          rdfs:subClassOf :Oven ;
          rdfs:comment "clay oven" ;
          rdfs:label "ClayOven" .


###  https://w3id.org/adso/ontology/ClayStill
:ClayStill rdf:type owl:Class ;
           rdfs:subClassOf :Distiller ;
           rdfs:comment "clay till" ;
           rdfs:label "ClayStill" .


###  https://w3id.org/adso/ontology/ClayVat
:ClayVat rdf:type owl:Class ;
         rdfs:subClassOf :FermentationVat ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :material ;
                           owl:someValuesFrom :Clay
                         ] ;
         rdfs:comment "clay fermentation vat" ;
         rdfs:label "ClayVat" .


###  https://w3id.org/adso/ontology/ColumnStill
:ColumnStill rdf:type owl:Class ;
